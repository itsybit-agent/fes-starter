{
  "$schema": "http://json.schemastore.org/template",
  "author": "itsyBIT",
  "classifications": ["Web", "API", "Event Sourcing", "CQRS", "Aspire"],
  "identity": "FesStarter.Template",
  "name": "FES Starter - Event Sourcing with Vertical Slices",
  "shortName": "fes",
  "description": "Full-stack starter with FileEventStore, CQRS, vertical slices, Aspire, and Angular",
  "tags": {
    "language": "C#",
    "type": "solution"
  },
  "sourceName": "FesStarter",
  "preferNameDirectory": true,
  "symbols": {
    "kebabName": {
      "type": "generated",
      "generator": "casing",
      "parameters": {
        "source": "name",
        "toLower": true
      },
      "replaces": "fes-starter"
    }
  },
  "sources": [
    {
      "modifiers": [
        {
          "exclude": [
            "**/bin/**",
            "**/obj/**",
            "**/.git/**",
            "**/node_modules/**",
            "**/.angular/**",
            "**/dist/**",
            "**/*.user",
            "**/.vs/**",
            "**/package-lock.json",
            "**/docs/CLAUDE-TEMPLATE.md"
          ]
        }
      ]
    }
  ],
  "postActions": []
}
